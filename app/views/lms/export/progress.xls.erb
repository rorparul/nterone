<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Index</Data></Cell>
        <Cell><Data ss:Type="String">Student name</Data></Cell>
        <Cell><Data ss:Type="String">Type</Data></Cell>
        <Cell><Data ss:Type="String">Name</Data></Cell>
        <Cell><Data ss:Type="String">Status</Data></Cell>
      </Row>
    <% @resources.each do |resource| %>
      <Row>
        <Cell><Data ss:Type="String"><%= resource[0] %></Data></Cell>
        <Cell><Data ss:Type="String"><%= resource[1] %></Data></Cell>
        <Cell><Data ss:Type="String"><%= resource[2] %></Data></Cell>
        <Cell><Data ss:Type="String"><%= resource[3] %></Data></Cell>
        <Cell><Data ss:Type="String"><%= resource[4] %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
